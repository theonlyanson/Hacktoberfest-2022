#include <stdio.h>
#include <string.h>

int main()
{
  char animals[5][20] = {{"pig"},{"cat"},{"dog"},{"cow"},{"tiger"}};
   printf("\nInitial order of the Animals: \n");
   for (int i = 0; i < 5; ++i)
   {
      printf("%s\n", s[i]);
   }

  char temp[50];
   for (int i = 0; i < 5; ++i)
   {
      for (int j = i + 1; j < 5; ++j)
      {
         if (strcmp(s[i], s[j]) > 0)
         {
            strcpy(temp, s[i]);
            strcpy(s[i], s[j]);
            strcpy(s[j], temp);
         }
      }
   }
   printf("\nAnimals in the lexicographical order: \n");
   for (int i = 0; i < 5; ++i)
   {
      printf("%s\n", s[i]);
   }

   return 0;
}

